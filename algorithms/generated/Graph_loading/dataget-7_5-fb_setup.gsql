drop all
 
create vertex Node (primary_id id string)
create directed edge Relation(from Node, to Node, weight FLOAT)
create graph datagen75(Node, Relation)

USE GRAPH datagen75
CREATE LOADING JOB load_datagen75_edge FOR GRAPH datagen75 {
   DEFINE FILENAME file1;

   LOAD file1 
              TO EDGE Relation VALUES ($0, $1, $2) USING header="false", separator=" ";
}

